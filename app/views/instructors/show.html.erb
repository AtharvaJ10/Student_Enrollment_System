<b>Name:</b> <%= @user.name %> <br>
<b>Email:</b> <%= @user.email %>
<%= render @instructor %>
<%if current_user.user_type!='Instructor' && current_user.user_type!='Student' %>
  <div>
    <%= link_to "Edit this instructor", edit_instructor_path(@instructor) %> |
    <%= link_to "Back to instructors", instructors_path %>

    <%= button_to "Destroy this instructor", @instructor, method: :delete %>
  </div>
<% end %>
